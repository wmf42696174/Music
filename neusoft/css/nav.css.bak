		.nav li .dw1 h3{
			height: 30px;
			line-height: 30px;
			font-size: 12px;
			color: #38c;
			text-indent: 20px;
			background-color: white;
		}
		.nav li .dw1 h3 a{
			color: #38c;
			text-decoration: none;
		}
		.nav li .dw1 h3 a:hover{
			text-decoration: underline;
		}
		.nav li .dw1 .solution_list{
			width:600px;
			height:90px;
			clear: both;
			
		}
		.nav li .dw1 .solution_list ul{
			float: left;
			width:190px;
			line-height: 20px;		
			border-right: 1px #E0E0E0 solid;
			margin: 0px;
			padding: 0px;
		}
		.nav li .dw1 .solution_list ul.solution_right{
			border: none;
		}
		.nav li .dw1 .solution_list ul li{
			height:22px;
			line-height: 17px;
			font-size: 12px;
			
			background-color: white;
			font-weight: normal;
		}
		.nav li .dw1 .solution_list ul li a{
			text-decoration: none;
			color:#555;
			display: block;
			width: 100px;
			height:22px;
			text-align: center;
		}
		.nav li .dw1 .solution_list ul li a:hover{
			text-decoration: underline;
		}
	
		.nav li .dw2{
			width:525px;
			height:330px;
			border-bottom:5px #0188CC solid;
			display: none;
			background:#fff;
			
		}
		.nav li.l3:hover .dw2{
			display: block;
		}
		.nav li .dw2 .product_list{
			width:525px;
			height:290px;
			
		}
		.nav li .dw2 .product_list ul li{
			width:100%;
			height:22px;
			line-height: 17px;
			padding-left: 10px;
		}
		.nav li .dw2 .product_list ul li a{
			color:#666;
			text-decoration: none;
			font-weight: normal;
		}
		.nav li .dw2 .product_list ul li a:hover{
			text-decoration: underline;
		}
		.nav li .dw2 .product_list .product_left{
			width:250px;
			border-right:1px solid #E0E0E0;
			float: left;
			line-height: 20px;
			height: 100%;
			margin: 0px;
			padding: 0px;
		}
		.nav li .dw2 .product_list .product_right{
			width:230px;
			float: left;
			line-height: 20px;
			height: 100%;
			margin: 0px;
			padding: 0px;
		}
		.nav li .dw3{
			width:705px;
			height:265px;
			border:1px solid #3388CC;
			left: -159px;
			display: none;
		}