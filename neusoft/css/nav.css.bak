.box .fen_nav{
			width:900px;
			height: 31px;
			margin:0 auto;
			background: url(http://www.neusoft.com/cn/newstyle/images/fen_nav.gif) repeat-x;
		}
		.box .fen_nav .nav{
			width:700px;
			height:30px;
			list-style: none;
			margin: 0px;
			padding: 0px;
		}
		.box .fen_nav .nav li{
			display: inline-block;
			width:110px;
			height:30px;
			font: bold 12px/30px Verdana, Geneva, sans-serif;
			position:relative;
		}
		.box .fen_nav .nav li .abutton{
			display: block;
			width: 100%;
			height: 25px;
			text-align: center;
			line-height: 30px;
			color: #666;
			border-bottom: 4px solid #666;
			text-decoration: none;
		}
			.box .fen_nav .nav li a.bluebutton{
			border-bottom: 4px solid #338BDD;
		}
		.nav li .dounmenu{
			position: absolute;
			top: 30px;
		}
		.nav li .dw1{
			width:615px;
			height:302px;
			border-bottom:5px #0188CC solid;
		  display: none;
		  background:#fff;
		  z-index:999;
			
		}
		.nav li.l2:hover .dw1{
			display: block;
		}
		.nav li .dounmenu h2{
			margin: 0px;
			padding: 0px;
			height: 30px;
			text-indent: 20px;
			background-color: #F0F0F0;
			font-size: 12px;
		}
		.nav li .dounmenu h2 a{
			color: #666;
			text-decoration: none;
			font-weight: normal;
		}
		.nav li .dounmenu h2 a:hover{
			text-decoration: underline;
		}
